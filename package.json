{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "Fork this repo to start a new project with typescript, prettier, eslint, and dotenv",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src -e ts,json --exec ts-node 'src/index'",
    "lint": "eslint 'src/**/*.ts'",
    "start": "node dist/index",
    "resizeBatch": "ts-node ./src/index.ts"
  },
  "keywords": [],
  "author": "beastman.kojak",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "mkdirp": "^3.0.1",
    "radash": "^11.0.0",
    "sharp": "^0.32.3"
  }
}
